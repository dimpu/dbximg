{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": [
        100
      ]
    },
    "interface-name": false,
    "no-console": false,
    "prettier": true,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
